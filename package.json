{
  "name": "vemorize-backend",
  "version": "0.1.0",
  "description": "Supabase-based backend for the Vemorize voice memorization app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:functions": "supabase functions serve --env-file .env",
    "dev:bootstrap": "./scripts/dev/bootstrap.sh",
    "dev:token": "./scripts/dev/get-local-token.sh",
    "dev:supabase": "supabase start",
    "db:reset": "supabase db reset",
    "db:migrate": "./scripts/db/migrate.sh",
    "db:seed": "npm run dev:bootstrap",
    "test:chat": "./scripts/test/test-chat-llm.sh",
    "test:nav": "./scripts/test/test-navigation.sh",
    "template:generate": "tsx scripts/templates/generate.ts",
    "template:save": "tsx scripts/templates/save.ts",
    "course:create": "tsx scripts/courses/create-from-template.ts",
    "agent:template": "tsx lab/agents/template-generator/cli.ts generate",
    "deploy": "./scripts/deploy/deploy.sh",
    "deploy:functions": "supabase functions deploy",
    "setup": "npm run db:reset && npm run dev:bootstrap",
    "fresh-start": "supabase db reset && npm run dev:bootstrap"
  },
  "keywords": [
    "vemorize",
    "supabase",
    "edge-functions",
    "deno"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.2.74",
    "@langchain/langgraph-checkpoint": "^0.0.18",
    "@langchain/openai": "^0.3.17",
    "@supabase/supabase-js": "^2.76.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.7",
    "tsx": "^4.20.0",
    "yaml": "^2.7.0",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "typescript": "^5.7.0"
  }
}
